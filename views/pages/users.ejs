<!DOCTYPE html>
<html lang="en">
<head>
  <title>user</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

<a href='http://localhost:5000/createuser'>createuser</a>
</head> 
<body>

<div class="container">
 
  
  <table class="table"   >
    <thead>
      <tr>
        <th>Name</th>
        <th>mobile</th>
        <th>Email</th>
        <th>type</th>
         
        <th>Action</th>
        

      </tr>
    </thead>
    <tbody>
      <% 
      result.forEach(function(result,i){ %>
        <tr> 
           <td><%- result.name %></td> 
           <td><%- result.mobile %></td>
           <td><%- result.email %></td>
           <td><%- result.type %></td>
          
          <td>
          <a href='http://localhost:5000/transaction/<%= result._id %>' ><button type="button" class="btn btn-primary">deposit</button></a>
          <a href='http://localhost:5000/transaction/<%= result._id %>' ><button type="button" class="btn btn-primary">withdraw</button></a>
          <a href='http://localhost:5000/transfer'><button type="button" class="btn btn-success">Transfer</button></a>
          </td>
          </tr>
          <% } ) %>
    </tbody>
  </table>
</div>

</body>
</html>
